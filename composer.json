{
    "name": "ardaasevinc/filament-activity-log",
    "description": "Laravel + Filament için sade ve güçlü activity log paketi",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Arda Sevinç",
            "email": "ardaasevinc@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0",
        "filament/filament": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "ArdaSevinc\\FilamentActivityLog\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ArdaSevinc\\FilamentActivityLog\\Providers\\FilamentActivityLogServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
